<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Warna background -->
    <solid android:color="#FFFFFF" />

    <!-- Border -->
    <stroke
        android:width="1dp"
        android:color="#662B5E" />

    <!-- Sudut melengkung -->
    <corners android:radius="12dp" />

    <!-- Padding di dalam -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />
</shape>
